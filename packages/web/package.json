{
  "name": "web",
  "version": "1.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "repository": "git@github.com:AleksandrHovhannisyan/aleksandrhovhannisyan.com.git",
  "author": "AleksandrHovhannisyan <aleksandrhovhannisyan@gmail.com>",
  "scripts": {
    "clean:all": "pnpm run clean && rm -rf .cache",
    "clean": "rm -rf dist",
    "debug": "DEBUG=Eleventy* ELEVENTY_ENV=development eleventy --serve --incremental --port=4001",
    "dev": "pnpm run clean && pnpm -F web --parallel --aggregate-output /^dev:/",
    "dev:html": "ELEVENTY_ENV=development eleventy --serve --incremental --port=4001",
    "dev:css": "sass --watch src/assets/styles:dist/assets/styles --style=expanded",
    "build": "pnpm run test && pnpm run clean && pnpm run build:html && pnpm run build:css",
    "build:html": "ELEVENTY_ENV=production eleventy",
    "build:css": "sass src/assets/styles:dist/assets/styles --style=compressed",
    "test": "node --test",
    "test:watch": "pnpm run test --watch",
    "lint": "pnpm run /^lint:/",
    "lint:css": "stylelint --fix \"src/**/*.scss\"",
    "lint:js": "eslint --cache --fix \"**/*.js\"",
    "update-browserslist": "pnpm dlx browserslist@latest --update-db"
  },
  "dependencies": {
    "@11ty/eleventy": "3.0.0",
    "@11ty/eleventy-cache-assets": "2.3.0",
    "@11ty/eleventy-img": "5.0.0-beta.10",
    "@iktakahiro/markdown-it-katex": "4.0.1",
    "@toycode/markdown-it-class": "1.2.4",
    "classnames": "2.3.1",
    "clean-css": "5.3.1",
    "eleventy-plugin-code-demo": "2.0.3",
    "eleventy-plugin-footnotes": "0.11.0",
    "esbuild": "0.14.37",
    "feather-icons": "4.28.0",
    "markdown-it": "12.0.6",
    "markdown-it-anchor": "8.0.3",
    "markdown-it-attrs": "4.1.3",
    "markdown-it-code-tabindex": "0.0.1",
    "markdown-it-link-attributes": "3.0.0",
    "markdown-it-toc-done-right": "4.2.0",
    "prismjs": "1.29.0",
    "sanitize-html": "2.6.1",
    "sass": "1.69.5",
    "slugify": "1.5.3",
    "stringify-attributes": "4.0.0",
    "stylelint": "14.7.1",
    "stylelint-config-standard-scss": "3.0.0",
    "zod": "3.24.1",
    "zod-validation-error": "3.4.0"
  },
  "lint-staged": {
    "*.js": "pnpm run lint:js",
    "*.scss": "pnpm run lint:css"
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 3 safari versions",
    "last 2 firefox versions",
    "last 2 edge versions"
  ]
}

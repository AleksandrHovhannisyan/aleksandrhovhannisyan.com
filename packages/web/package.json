{
  "name": "web",
  "version": "1.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "repository": "git@github.com:AleksandrHovhannisyan/aleksandrhovhannisyan.com.git",
  "author": "AleksandrHovhannisyan <aleksandrhovhannisyan@gmail.com>",
  "scripts": {
    "clean:all": "pnpm run clean && rm -rf .cache",
    "clean": "rm -rf dist",
    "debug": "DEBUG=Eleventy* ELEVENTY_ENV=development eleventy --serve --incremental --port=4001",
    "dev": "pnpm run clean && pnpm -F web --parallel --aggregate-output /^dev:/",
    "dev:html": "ELEVENTY_ENV=development eleventy --config=.eleventy.ts --serve --incremental --port=4001",
    "build": "pnpm run test && pnpm run clean && ELEVENTY_ENV=production eleventy --config=.eleventy.ts",
    "test": "node --test",
    "test:watch": "pnpm run test --watch",
    "lint": "pnpm run /^lint:/",
    "lint:css": "prettier \"src/**/*.css\" --write",
    "lint:js": "eslint --cache --fix"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-img": "^6.0.4",
    "@iktakahiro/markdown-it-katex": "^4.0.1",
    "@toycode/markdown-it-class": "^1.2.4",
    "classnames": "^2.3.1",
    "eleventy-plugin-code-demo": "^2.0.8",
    "eleventy-plugin-footnotes": "^0.11.0",
    "esbuild": "^0.14.37",
    "feather-icons": "^4.28.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^8.0.3",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-code-tabindex": "^1.0.1",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.2.4",
    "prismjs": "^1.29.0",
    "sanitize-html": "^2.6.1",
    "slugify": "^1.5.3",
    "stringify-attributes": "^4.0.0",
    "valibot": "~1.0.0-rc.3"
  },
  "lint-staged": {
    "*.js": "pnpm run lint:js",
    "*.css": "pnpm run lint:css"
  },
  "browserslist": [
    ">0.3%",
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "last 3 Safari versions",
    "last 3 Edge versions",
    "not dead",
    "not op_mini all",
    "not IE > 0"
  ],
  "devDependencies": {
    "@types/feather-icons": "^4.29.4",
    "@types/markdown-it": "^14.1.2"
  }
}

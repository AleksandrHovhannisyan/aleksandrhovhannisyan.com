{
  "name": "Yada 11ty Theme",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [ "json-ld", "11ty", "sanity.io", "blog", "11ty themes" ],
  "repository": {
    "type": "git",
    "url" : "git@github.com:denverprophitjr/yada-11ty.git"
  },
  "author": "Denver Prophit Jr. <develop@denverprophit.us>",
  "homepage": "https://github.com/denverprophitjr/yada-11ty#readme",
  "bugs": {
		"url": "http://github.com/denverprophitjr/yada-11ty/issues",
	},
  "author": {
		"name": "Denver Prophit Jr.",
		"email": "develop@denverprophit.us",
		"url": "https://denverprophit.us/"
  },
    // An array of contributors to the package.
	"contributors": [
		{
			"name": "Aleksandr Hovhannisyan",
			"email": "aleksandrhovhannisyan@gmail.com",
			"url": "https://github.com/AleksandrHovhannisyan/aleksandrhovhannisyan.com"
		},
  "scripts": {
    "clean:all": "rimraf _site",
    "clean": "rimraf '_site/!(assets)' '_site/assets/!(images)'",
    "debug:eleventy": "DEBUG=Eleventy* ELEVENTY_ENV=development npx @11ty/eleventy --serve --incremental --port=4001",
    "serve:webpack": "webpack --mode=development --watch",
    "serve:sass": "sass --watch src/assets/styles:_site/assets/styles --style=expanded",
    "serve:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --incremental --port=4001",
    "serve": "npm-run-all clean --parallel serve:*",
    "build:webpack": "webpack --mode=production",
    "build:sass": "sass src/assets/styles:_site/assets/styles",
    "build:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "build": "run-s test clean build:eleventy build:sass build:webpack",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:css:fix": "stylelint --fix \"src/**/*.scss\"",
    "lint:js": "eslint --cache \"**/*.js\"",
    "lint:js:fix": "eslint --cache --fix \"**/*.js\"",
    "lint": "npm-run-all lint:*",
    "lint:fix": "npm-run-all --parallel lint:*:fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.cjs",
    "test:watch": "yarn test --watch --silent=false --silent=false",
    "update-browserslist": "npx browserslist@latest --update-db"
  },
  "dependencies": {
    "dayjs": "^1.10.5",
    "dompurify": "^2.2.8",
    "marked": "^2.0.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "1.0.0-beta.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@iktakahiro/markdown-it-katex": "^4.0.1",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "feather-icons": "^4.28.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^8.0.3",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "netlify-plugin-11ty": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "simple-icons": "^5.3.0",
    "slugify": "^1.5.3",
    "stylelint": "^13.12.0",
    "stylelint-scss": "^3.19.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "browserslist": [
    "defaults and supports es6-module and not IE <= 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarn"
    }
  },
  "lint-staged": {
    "*.js": "yarn run lint:js:fix",
    "*.scss": "yarn run lint:css:fix"
  }
}

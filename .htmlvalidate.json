{
    "extends": ["html-validate:recommended"],
    "rules": {
        "no-trailing-whitespace": "off",
        "long-title": "warn",
        "svg-focusable": "off",
        "no-inline-style": "off",
        "void-style": "off",
        "no-raw-characters": "warn",
        "attribute-boolean-style": "warn",
        "wcag/h30": "warn"
    }
}